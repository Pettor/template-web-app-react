{
  "name": "web",
  "private": true,
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf dist storybook-static .nyc_output .junit coverage",
    "clean:all": "pnpm clean node_modules",
    "dev": "vite --open",
    "dev:mocks": "vite --mode mocks --open",
    "lint": "eslint ./src ./.storybook --ext .js,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "test": "test-storybook --url http://127.0.0.1:9050",
    "test:ci": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9055 \"pnpm storybook:test --junit --coverage\"",
    "test:unit": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9055 \"pnpm storybook:test --junit\"",
    "test:coverage": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9055 \"pnpm storybook:test --coverage\"",
    "test:coverage:create": "npx nyc report --reporter=cobertura -t .nyc_output --report-dir .nyc_output",
    "test:coverage:move": "move-cli .nyc_output/cobertura-coverage.xml \"../../output/coverage/web.xml\" --mkdirp",
    "storybook:dev": "storybook dev -p 9050",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:move": "move-cli storybook-static \"../../output/storybook-static\" --mkdirp",
    "storybook:server": "npx http-server storybook-static --port 9055 --silent",
    "storybook:test": "test-storybook --url http://127.0.0.1:9055"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.1",
    "@tanstack/react-query": "4.29.23",
    "axios": "1.4.0",
    "axios-auth-refresh": "3.3.6",
    "immer": "10.0.2",
    "mui-theme": "workspace:*",
    "react-hook-form": "7.45.1",
    "react-router-dom": "6.14.1",
    "react-utils": "workspace:*",
    "shared-ui": "workspace:*",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint-config-custom": "workspace:*",
    "http-server": "14.1.1",
    "start-server-and-test": "2.0.0",
    "storybook-base": "workspace:*",
    "tsconfig": "workspace:*",
    "vite-plugin-http2-proxy": "0.4.1",
    "vite-plugin-mkcert": "1.16.0",
    "vite-plugin-pwa": "0.16.4",
    "vite-tsconfig-paths": "4.2.0",
    "workbox-window": "7.0.0"
  },
  "jest-junit": {
    "suiteName": "web tests",
    "outputDirectory": ".junit",
    "outputName": "web.xml"
  }
}
