    "mui-theme": "workspace:*",
    "react-hook-form": "7.45.1",
    "react-router-dom": "6.14.1",
    "react-utils": "workspace:*",
    "shared-ui": "workspace:*",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@vitejs/plugin-react": "4.0.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint-config-custom": "workspace:*",
    "http-server": "14.1.1",
