    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-storybook": "0.6.12",
    "http-server": "14.1.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "storybook": "7.0.20",
    "typescript": "5.1.3",
