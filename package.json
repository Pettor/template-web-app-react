{
  "name": "template-web-app-react",
  "version": "0.4.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build --no-daemon",
    "clean": "turbo run clean --no-daemon && rimraf output",
    "clean:all": "turbo run clean:all --no-daemon",
    "dev": "turbo run dev --no-daemon",
    "dev:mocks": "turbo run dev:mocks --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --no-daemon",
    "lint:fix": "turbo run lint:fix --no-daemon",
    "test": "turbo run test --parallel --no-daemon",
    "test:ci": "turbo run test:ci --parallel --no-daemon",
    "mocks": "pnpm --filter=mock mocks",
    "storybook": "turbo run storybook:dev --parallel --no-daemon",
    "storybook:dev:combined": "concurrently -k -s first -n \"ui,web\" -c \"magenta,blue\" \"pnpm storybook:dev:ui\" \"pnpm wait-on http://localhost:6006 && pnpm storybook:dev:web\"",
    "storybook:dev:web": "turbo run storybook:dev --filter=web --no-daemon",
    "storybook:dev:ui": "turbo run storybook:dev --filter=shared-ui --no-daemon",
    "storybook:build": "turbo run storybook:build --parallel --no-daemon",
    "storybook:move": "pnpm storybook:move:web && pnpm storybook:move:ui",
    "storybook:move:web": "turbo run storybook:move --filter=web --no-daemon",
    "storybook:move:ui": "turbo run storybook:move --filter=shared-ui --no-daemon"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.13.7",
    "@mui/lab": "5.0.0-alpha.135",
    "@mui/material": "5.13.7",
    "concurrently": "8.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "wait-on": "7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@commitlint/types": "17.4.4",
    "@mocks-server/main": "4.1.0",
    "@storybook/addon-a11y": "7.0.26",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-docs": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-styling": "1.3.2",
    "@storybook/addon-viewport": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/react-vite": "7.0.26",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/test-runner": "0.11.0",
    "@storybook/testing-library": "0.2.0",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "move-cli": "2.0.0",
    "playwright": "1.35.1",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "start-server-and-test": "2.0.0",
    "storybook": "7.0.26",
    "turbo": "1.10.7",
    "typescript": "5.1.6",
    "usehooks-ts": "2.9.1",
    "vite": "4.4.2"
  }
}
