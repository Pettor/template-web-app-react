{
  "name": "template-web-app-react",
  "version": "0.4.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build --no-daemon",
    "clean": "turbo run clean --no-daemon",
    "clean:all": "turbo run clean:all --no-daemon",
    "dev": "turbo run dev --no-daemon",
    "dev:mocks": "turbo run dev:mocks --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --no-daemon",
    "lint:fix": "turbo run lint:fix --no-daemon",
    "test": "turbo run test --no-daemon",
    "mocks": "pnpm --filter=mock mocks",
    "storybook": "concurrently -k -s first -n \"ui,web\" -c \"magenta,blue\" \"pnpm storybook:ui\" \"pnpm wait-on http://localhost:6006 && pnpm storybook:web\"",
    "storybook:web": "turbo run storybook --filter=web",
    "storybook:ui": "turbo run storybook --filter=shared-ui",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:move": "pnpm storybook:move:web && pnpm storybook:move:ui",
    "storybook:move:web": "turbo run storybook:move --filter=web",
    "storybook:move:ui": "turbo run storybook:move --filter=shared-ui"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.134",
    "@mui/material": "5.13.6",
    "concurrently": "8.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "wait-on": "7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@commitlint/types": "17.4.4",
    "@mocks-server/main": "4.1.0",
    "@storybook/addon-a11y": "7.0.24",
    "@storybook/addon-actions": "7.0.24",
    "@storybook/addon-docs": "7.0.24",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-interactions": "7.0.24",
    "@storybook/addon-styling": "1.3.2",
    "@storybook/addon-viewport": "7.0.24",
    "@storybook/react": "7.0.24",
    "@storybook/react-vite": "7.0.24",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/test-runner": "0.11.0",
    "@storybook/testing-library": "0.2.0",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "husky": "8.0.3",
    "move-cli": "2.0.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "storybook": "7.0.24",
    "turbo": "1.10.7",
    "typescript": "5.1.6",
    "usehooks-ts": "^2.9.1"
  }
}
