{
  "name": "template-web-app-react",
  "version": "0.6.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  },
  "scripts": {
    "prepare": "husky",
    "build": "turbo run build --no-daemon",
    "deploy": "turbo run deploy --no-daemon",
    "clean": "turbo run clean --no-daemon && rimraf output",
    "clean:all": "turbo run clean:all --no-daemon",
    "dev": "turbo run dev --no-daemon --concurrency 20",
    "dev:mocks": "turbo run dev:mocks --no-daemon --concurrency 20",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --no-daemon",
    "lint:fix": "turbo run lint:fix --no-daemon",
    "test": "turbo run test --parallel --no-daemon",
    "test:coverage": "turbo run test:coverage --parallel --no-daemon",
    "test:coverage:ci": "turbo run test:coverage:ci --parallel --no-daemon",
    "test:e2e": "concurrently -k -s \"command-e2e:headless\" -n \"e2e:headless,e2e:env\" -c \"magenta,blue\" \"pnpm test:e2e:headless\" \"pnpm test:e2e:env\"",
    "test:e2e:headless": "pnpm wait-on tcp:5173 && pnpm --filter=e2e test:e2e",
    "test:e2e:env": "turbo run test:e2e:env --parallel --no-daemon",
    "mocks": "pnpm --filter=mock mocks",
    "storybook": "pnpm storybook:dev:combined",
    "storybook:dev:combined": "turbo run storybook:dev:combined --filter=web --no-daemon",
    "storybook:dev:web": "turbo run storybook:dev --filter=web --no-daemon",
    "storybook:dev:ui": "turbo run storybook:dev --filter=ui-package --no-daemon",
    "storybook:build": "turbo run storybook:build --parallel --no-daemon",
    "storybook:publish": "turbo run storybook:publish --parallel --no-daemon",
    "storybook:move": "pnpm storybook:move:web && pnpm storybook:move:ui",
    "storybook:move:web": "turbo run storybook:move --filter=web --no-daemon",
    "storybook:move:ui": "turbo run storybook:move --filter=ui-package --no-daemon"
  },
  "dependencies": {
    "@fontsource/rubik": "5.2.6",
    "@heroicons/react": "2.2.0",
    "@tanstack/react-query": "5.87.1",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "clsx": "2.1.1",
    "concurrently": "9.2.1",
    "daisyui": "5.0.50",
    "immer": "10.1.1",
    "jotai": "2.13.1",
    "jotai-effect": "2.0.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-intl": "7.1.11",
    "wait-on": "8.0.4",
    "yup": "1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@faker-js/faker": "10.0.0",
    "@mocks-server/main": "4.1.0",
    "@storybook/addon-themes": "9.1.3",
    "@storybook/addon-vitest": "9.1.3",
    "@storybook/builder-vite": "9.1.3",
    "@storybook/react": "9.1.3",
    "@storybook/react-vite": "9.1.3",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.12",
    "@types/node": "22.18.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "cross-env": "10.0.0",
    "eslint": "9.34.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "move-cli": "2.0.0",
    "nyc": "17.1.0",
    "playwright": "1.55.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rimraf": "6.0.1",
    "storybook": "9.1.3",
    "tailwindcss": "4.1.12",
    "turbo": "2.5.6",
    "typescript": "5.8.3",
    "usehooks-ts": "3.1.1",
    "utility-types": "3.11.0",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.9.2",
      "vite": "7.1.4"
    }
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.ts": "prettier --write",
    "*.tsx": "prettier --write",
    "*.scss": "prettier --write"
  }
}
