{
  "name": "template-web-app-react",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "i18n-extract": "formatjs extract src/**/*.ts* --ignore=**/*.d.ts --out-file locale/extracted/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --flatten",
    "i18n-compile": "formatjs compile-folder --ast locale/extracted locale/compiled"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.6",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.10.14",
    "@mui/lab": "^5.0.0-alpha.108",
    "@mui/material": "^5.10.14",
    "axios": "^0.26.0",
    "buffer": "^6.0.3",
    "idb-keyval": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.29.0",
    "react-intl": "^6.1.1",
    "react-router-dom": "^6.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@formatjs/cli": "^5.1.0",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.12",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/test-runner": "^0.7.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^2.2.0",
    "concurrently": "^7.2.2",
    "emotion-theming": "^11.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-formatjs": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "http-server": "^14.1.1",
    "jest": "27",
    "prettier": "^2.5.1",
    "storybook-dark-mode": "^1.0.9",
    "typescript": "^4.7.4",
    "vite": "^3.2.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.64.4"
  }
}
