{
  "name": "template-web-app-react",
  "version": "0.4.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.6.1",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache  --continue --concurrency 20",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "storybook": "concurrently -k -s first -n \"ui,web\" -c \"magenta,blue\" \"pnpm storybook:ui\" \"pnpm wait-on http://localhost:6006 && pnpm storybook:web\"",
    "storybook:web": "turbo run storybook --filter=web",
    "storybook:ui": "turbo run storybook --filter=shared-ui",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:move": "pnpm storybook:move:web && pnpm storybook:move:ui",
    "storybook:move:web": "turbo run storybook:move --filter=web",
    "storybook:move:ui": "turbo run storybook:move --filter=shared-ui"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.133",
    "@mui/material": "5.13.4",
    "concurrently": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.3",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.0.20",
    "@storybook/addon-actions": "7.0.20",
    "@storybook/addon-docs": "7.0.20",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-styling": "1.1.0",
    "@storybook/addon-viewport": "7.0.20",
    "@storybook/react": "7.0.20",
    "@storybook/react-vite": "7.0.20",
    "@storybook/storybook-deployer": "2.8.16",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "move-cli": "2.0.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "storybook": "7.0.20",
    "tsup": "6.7.0",
    "turbo": "1.10.2",
    "typescript": "5.1.3"
  }
}
