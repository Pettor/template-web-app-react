    "@mui/material": "5.13.1",
    "@tanstack/react-query": "4.29.7",
    "axios": "1.4.0",
    "axios-auth-refresh": "3.3.6",
    "buffer": "6.0.3",
    "idb-keyval": "6.2.1",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.4.2",
    "react-router-dom": "6.11.1",
    "yup": "1.1.1"
