{
  "name": "shared-ui",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules",
    "lint": "eslint ./src ./.storybook --ext .js,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "test": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9060 \"pnpm storybook:test\"",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:move": "move-cli storybook-static \"../../storybook-static/shared-ui\"",
    "storybook:server": "npx http-server storybook-static --port 9060 --silent",
    "storybook:test": "test-storybook --url http://127.0.0.1:9060"
  },
  "dependencies": {
    "mui-theme": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "react-utils": "workspace:*",
    "storybook-base": "workspace:*",
    "tsconfig": "workspace:*"
  }
}
