{
  "name": "shared-ui",
  "version": "0.5.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist storybook-static .nyc_output coverage",
    "clean:all": "pnpm clean node_modules",
    "lint": "eslint ./src ./.storybook --ext .js,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "test": "test-storybook --url http://127.0.0.1:9060",
    "test:ci": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9065 \"pnpm storybook:test\"",
    "storybook:dev": "storybook dev -p 9060 --quiet",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:move": "move-cli storybook-static \"../../output/storybook-static/shared-ui\"",
    "storybook:server": "npx http-server storybook-static --port 9065 --silent",
    "storybook:test": "test-storybook --url http://127.0.0.1:9065 --junit"
  },
  "dependencies": {
    "mui-theme": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "react-utils": "workspace:*",
    "storybook-base": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "jest-junit": {
    "suiteName": "shared-ui tests",
    "outputDirectory": "../../output/junit",
    "outputName": "shared-ui.xml"
  }
}
