{
  "name": "shared-ui",
  "version": "0.5.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist storybook-static .nyc_output coverage",
    "clean:all": "pnpm clean node_modules",
    "lint": "eslint ./src ./.storybook --ext .js,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "test": "test-storybook --url http://127.0.0.1:9060",
    "test:unit": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9065 \"pnpm storybook:test --junit\"",
    "test:coverage": "start-server-and-test \"pnpm storybook:server\" http://127.0.0.1:9065 \"pnpm storybook:test --coverage\"",
    "test:coverage:report": "npx nyc report --reporter=cobertura -t .nyc_output --report-dir .nyc_output",
    "storybook:dev": "storybook dev -p 9060 --quiet",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:move": "move-cli storybook-static \"../../output/storybook-static/shared-ui\" --mkdirp",
    "storybook:server": "npx http-server storybook-static --port 9065 --silent",
    "storybook:test": "test-storybook --url http://127.0.0.1:9065"
  },
  "dependencies": {
    "mui-theme": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "react-utils": "workspace:*",
    "storybook-base": "workspace:*",
    "tsconfig": "workspace:*",
    "vite-tsconfig-paths": "4.2.0"
  },
  "jest-junit": {
    "suiteName": "shared-ui tests",
    "outputDirectory": "junit",
    "outputName": "shared-ui.xml"
  }
}
