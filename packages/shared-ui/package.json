{
  "name": "shared-ui",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "pnpm lint --fix",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --verbose",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:move": "move-cli storybook-static \"../../storybook-static/shared-ui\""
  },
  "dependencies": {
    "mui-theme": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "react-utils": "workspace:*",
    "storybook-base": "workspace:*",
    "tsconfig": "workspace:*"
  }
}
